{
  "name": "snake",
  "version": "1.0.0",
  "description": "My attempt at building snake in ts",
  "main": "index.ts",
  "scripts": {
    "dist": "mkdir docs && cd docs && mkdir docs && mkdir game && cd game && mkdir css && mkdir src && cd .. && cd .. && cp dist/es5/* docs/game/src && cp ./*.html docs/game && cp css/* docs/game/css",
    "test": "jest",
    "rollup": "rollup -c",
    "test-watch": "jest --watch",
    "clean": "rimraf -f dist && rimraf -f docs",
    "typescript": "tsc --build tsconfig.json",
    "terser": "terser dist/es6/snake.js -c -m -o dist/es6/snake.js",
    "css-build": "node-sass --omit-source-map-url sass/styles.scss css/styles.css",
    "css-watch": "npm run css-build -- --watch",
    "build": "npm run clean && npm run typescript && npm run rollup && npm run terser && npm run dist",
    "start": "npm run css-watch"
  },
  "repository": {
    "type": "git",
    "url": "github.com/blackeuler/snake"
  },
  "keywords": [
    "snake",
    "game",
    "fun"
  ],
  "author": "monadicblack",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.19",
    "bulma": "^0.9.1",
    "jest": "^26.6.3",
    "node-sass": "^5.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.34.2",
    "terser": "^5.5.1",
    "ts-jest": "^26.4.4",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.3"
  }
}
